<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport"
          content="minimal-ui, user-scalable=no, width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="HandheldFriendly" content="True">
    <meta name="mobile-web-app-title" content="react组件">
    <meta name="apple-mobile-web-app-title" content="react组件">
    <%for (var css in htmlWebpackPlugin.options.addLinkCss) {%>
        <link rel="stylesheet" href="<%= htmlWebpackPlugin.options.addLinkCss[css] %>">
    <%}%>
    <%for (var js in htmlWebpackPlugin.options.addmommonScript) {%>
    <script src="<%= htmlWebpackPlugin.options.addmommonScript[js] %>"></script>
    <%}%>
    <link rel="stylesheet" href="/styles/login.css" />
</head>
<body>
    <div class="login-view">
        <div class="login-content">
            <div class="login-header">
                <h1>组件管理系统</h1>
            </div>
            <div class="login-center">
                <div id="example"></div>
            </div>
        </div>
    </div>
    <div class="login-bg"></div>
</body>
</html>
